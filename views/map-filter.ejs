<div class="card">
  <div class="card-header">
    Filter Map Content
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">
      <div class="form-check">
        <input type="checkbox" class="form-check-input" id="mapFilter.camera.input" checked>
        <label class="form-check-label" for="mapFilter.camera.input">
          Show Cameras
        </label>
      </div>
    </li>
    <li class="list-group-item">
      <div class="form-check">
        <input type="checkbox" class="form-check-input" id="mapFilter.ap.input" checked>
        <label class="form-check-label" for="mapFilter.ap.input">
          Show Access Points
        </label>
      </div>
    </li>
  </ul>
</div>
